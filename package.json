{
  "name": "react_template_project",
  "version": "0.0.3",
  "private": false,
  "scripts": {
    "build": "webpack --config build/webpack.config.prod.js",
    "analyse": "cross-env ANALYSE=1 npm run build",
    "build:dll": "rimraf static/dll/ && webpack --config build/webpack.config.dll.js",
    "clean": "rimraf dist",
    "dev": "cross-env NODE_ENV=development PORT=3000 node server.js",
    "start": "cross-env NODE_ENV=production node server.js",
    "devdb": "rimraf dist/* && cross-env SF_ENV=devdb npm run build",
    "sit": "rimraf dist/* && cross-env SF_ENV=sit npm run build",
    "prd": "rimraf dist/* && cross-env SF_ENV=prd npm run build",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 9",
    "not op_mini all"
  ],
  "dependencies": {
    "antd": "^4.15.3",
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "core-js": "^3.11.0",
    "dayjs": "^1.10.4",
    "moment": "^2.29.1",
    "prop-types": "^15.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "redux-spring": "^2.0.19"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/plugin-transform-runtime": "^7.13.15",
    "@babel/preset-env": "^7.14.0",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/react": "^17.0.4",
    "@types/react-router": "^5.1.13",
    "assets-webpack-plugin": "^7.1.0",
    "autoprefixer": "^10.2.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-webpack-dynamic-import": "^1.0.1",
    "compression": "^1.7.4",
    "copy-webpack-plugin": "^9.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "express": "^4.16.4",
    "file-loader": "^6.2.0",
    "forward-request": "^1.0.3",
    "html-webpack-plugin": "^5.3.1",
    "husky": "^6.0.0",
    "less": "^4.1.1",
    "less-loader": "^10.0.0",
    "lint-staged": "^11.0.0",
    "mini-css-extract-plugin": "^1.5.1",
    "postcss-loader": "^6.1.0",
    "rimraf": "^3.0.2",
    "style-loader": "^2.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.36.1",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.6.0",
    "webpack-dev-middleware": "^5.0.0",
    "webpack-hot-middleware": "^2.25.0"
  }
}
