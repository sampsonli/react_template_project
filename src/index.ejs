<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset=UTF-8>
    <title>我的页面</title>
    <meta name=apple-mobile-web-app-capable content=yes>
    <meta name=apple-mobile-web-app-status-bar-style content=black>
    <meta name=format-detection content="telephone=no">
    <meta name=applicable-device content=mobile>

    <meta name="viewport" content="width=device-width,minimum-scale=1,maximum-scale=1,user-scalable=no"/>
    <script type="text/javascript">
        var rFontSize;
        function resize() {
            var clientWith = document.documentElement.clientWidth;
            rFontSize = clientWith / 3.75;
            document.documentElement.style.fontSize = rFontSize + "px";
        }
        window.onresize = resize;
        resize();
        function rem2px (size) {
            return size * rFontSize;
        }
        function px2rem (size) {
            return size / rFontSize;
        }
    </script>
</head>
<body>
<div id="app"></div>
<% if (htmlWebpackPlugin.options.dllName) { %>
    <script src="<%=htmlWebpackPlugin.options.dllName%>"></script>
<% } %>
</body>
</html>
